Create Table Tutor ( tLastName varchar(30), tFirstName varchar(30), Email varchar(40), IsAdmin tinyint, Password varchar(30), Primary key(Email) );
Create Table Client (
LastName varchar(30) NOT NULL,
FirstName varchar(30) NOT NULL,
StudentID varchar(10) NOT NULL,
Major varchar(45) NOT NULL,
Year varchar(30) NOT NULL,
English tinyint,
EmailAddress varchar(40) NOT NULL,
Password varchar(30) NOT NULL,
Primary Key(StudentID) );
Create Table Client ( LastName varchar(30) NOT NULL, FirstName varchar(30) NOT NULL, StudentID varchar(10) NOT NULL, Major varchar(45) NOT NULL, Year varchar(30) NOT NULL, English tinyint, EmailAddress varchar(40) NOT NULL, Password varchar(30) NOT NULL, Primary Key(StudentID) );
Create Table Appointment (
Email varchar(40),
StudentID varchar(10),
StartTime datetime,
Duration int,
GroupSize int,
FirstTimeVisit tinyint,
Comment varchar(120),
HelpService varchar(35),
FOREIGN KEY (StudentID) references Client(StudentID),
FOREIGN KEY (Email) references Tutor(Email) );
Create Table Appointment ( Email varchar(40), StudentID varchar(10), StartTime datetime, Duration int, GroupSize int, FirstTimeVisit tinyint, Comment varchar(120), HelpService varchar(35), FOREIGN KEY (StudentID) references Client(StudentID), FOREIGN KEY (Email) references Tutor(Email) );
